            "name": "",
            "code": "// ajusta a temperatura para um n√∫mero inteiro\ncorrecaoTemperatura = Math.round(apiTemperatura || 0);\n\n// ajusta o case sensitive da mensagem de condi√ß√£o da api\nconst correcaoCondicaoTempo = (apiCondicaoTempo || \"\").toLowerCase();\n\n// l√≥gica para mensagem de condi√ß√£o do tempo\nswitch(correcaoCondicaoTempo) {\n    case 'rain':\n        comentarioCondicaoTempo = \"e est√° chovendo, melhor n√£o esquecer o guarda-chuva! ‚òî\";\n        break;\n    case 'drizzle':\n        comentarioCondicaoTempo = \"e pode chover um pouco, melhor n√£o esquecer o guarda-chuva! ‚òî\";\n        break;\n    case 'clear':\n        comentarioCondicaoTempo = \"e o c√©u est√° limpo e com poucas nuvens! ‚ú®\";\n        break;\n    case 'clouds':\n        comentarioCondicaoTempo = \"e est√° um pouco nublado. ‚òÅÔ∏è\";\n        break;\n    case 'thunderstorm':\n        comentarioCondicaoTempo = \"e aten√ß√£o para trovoadas! ‚õàÔ∏è\";\n        break;\n    default:\n        comentarioCondicaoTempo = \"e o tempo est√° excelente! ‚ú®\";\n}\n\n// l√≥gica para mensagem de temperatura\nif (correcaoCondicaoTempo === 'rain' || correcaoCondicaoTempo === 'thunderstorm') {\n    comentarioTemperatura = \"Ô∏è\";\n} else if (correcaoTemperatura < 18) {\n    comentarioTemperatura = \"Que frio, melhor levar um casaco, n√£o acha? üß•\";\n} else if (correcaoTemperatura > 28) {\n    comentarioTemperatura = \"Dia quente! N√£o esque√ßa de beber √°gua. ü•§\";\n} else {\n    comentarioTemperatura = \"O tempo est√° √≥timo! Nem muito frio, nem muito quente. Perfeito, n√£o acha? ‚ú®\";\n}",
